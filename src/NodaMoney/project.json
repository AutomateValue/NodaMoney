{
  "version": "1.0.0",
  "title": "NodaMoney",
  "description": "NodaMoney provides a library that treats Money as a first class citizen and handles all the ugly bits like currencies and formatting.",
  "language": "en-US",
  "authors": [ "Rémy van Duijkeren" ],
  "copyright": "Copyright Rémy van Duijkeren",
  "packOptions": {
    "projectUrl": "http://www.nodamoney.org/",
    "tags": [ "Noda", "Money", "Currency", "ExchangeRate" ],
    "licenseUrl": "https://github.com/remyvd/NodaMoney/blob/master/LICENSE.txt",
    "iconUrl": "https://raw.githubusercontent.com/remyvd/NodaMoney/master/docs/logo_nodamoney.png",
    "releaseNotes": "See https://github.com/remyvd/NodaMoney/releases/",
    "repository": {
      "type": "git",
      "url": "https://github.com/remyvd/NodaMoney"
    }
  },
  "buildOptions": {
    "xmlDoc": true
  },
  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "System.Diagnostics.Tools": "4.3.0"
      }
    },
    "netstandard1.0": {
      "dependencies": {
        "System.Diagnostics.Tools": "4.3.0",
        "System.Xml.XmlSerializer": "4.3.0"
      }
    },
    "net40": {
      "frameworkAssemblies": {
        "System.Runtime.Serialization": "4.0.0",
        "System.Xml": "4.0.0"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Runtime.Serialization": "4.0.0",
        "System.Xml": "4.0.0"
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "buildOptions": {
        "define": [ "PORTABLE" ]
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "buildOptions": {
        "define": [ "PORTABLE40" ]
      },
      "frameworkAssemblies": {
          "System.Runtime.Serialization": "2.0.5",
          "System.Xml": "2.0.5"
        }
    }
  }
}